{
    "version":2,
    "builds":[
        {
            "src":"index.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src": "/products",
            "dest": "index.js"
          },
          {
            "src": "/users",
            "dest": "index.js"
          },
          {
            "src": "/weather",
            "dest": "index.js"
          },
        {
            "src":"/(.*)",
            "dest":"index.js"
        }
    ]
}